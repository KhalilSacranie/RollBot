generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model sheetmodels {
  id      String  @id @default(auto()) @map("_id") @db.ObjectId
  sheetID String  @unique
  link    String?
}

model Users {
  id            String @id @default(auto()) @map("_id") @db.ObjectId
  characterName String
}
